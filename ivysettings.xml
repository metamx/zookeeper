<ivysettings>
  <properties environment="env" />

  <property
    name="repo.host"
    value="${env.REPO_HOST}"
    override="false"
    ifset="env.REPO_HOST" />
  <property name="repo.host" value="repo1.maven.org" override="false" />

  <property
    name="repo.root"
    value="${env.REPO_ROOT}"
    override="false"
    ifset="env.REPO_ROOT" />
  <property name="repo.root" value="http://repo1.maven.org/maven2/" override="false" />

  <property
    name="repo.passwd"
    value="${env.REPO_PASSWD}"
    override="false"
    ifset="env.REPO_PASSWD" />
  <property name="repo.passwd" value="" override="false" />

  <property
    name="repo.realm"
    value="${env.REPO_REALM}"
    override="false"
    ifset="env.REPO_REALM" />
  <property name="repo.realm" value="Artifactory Realm" override="false" />

  <property
    name="repo.username"
    value="${env.REPO_USERNAME}"
    override="false"
    ifset="env.REPO_USERNAME" />
  <property name="repo.username" value="" override="false" />

  <credentials
    host="${repo.host}"
    passwd="${repo.passwd}"
    realm="${repo.realm}"
    username="${repo.username}" />
  <settings defaultResolver="default" />
  <resolvers>
    <ibiblio
      name="default"
      root="${repo.root}"
      m2compatible="true"
      pattern="[organisation]/[module]/[revision]/[module]-[revision].[ext]" />
  </resolvers>
</ivysettings>
